{
    // Command
    "command": {
        // Is enable
        "enable": true,
        "name": "example",
        "description": "Example command", 
        // Command aliases
        "aliases": ["example1"],
        // Permission
        // Delete it if you don't have to
        "permission": "formcreator.example"
    },
    // Form title
    "title": "Example Form",
    // Form content
    "content": [
        "Content line"
    ],
    // Form buttons
    "buttons": [
        {
            "name": "Button",
            // Button actions
            "actions": [
                {
                    // Available types: 
                    // PLAYER_COMMAND - Execute command from player
                    // CONSOLE_COMMAND  - Execute command from server
                    // MESSAGE - Send message to player
                    // PLAYER_MESSAGE - Send message from player
                    // OPEN - Open other form
                    "type": "OPEN",   // Type
                    "data": "example" // Action data
                }
            ]
        }
    ],
    // Form open actions
    "openActions": [
        {
            "type": "CONSOLE_COMMAND",
            "data": "say {player} Form opened!"
        }
    ],
    // Form close actions
    "closeActions": [
        {
            "type": "CONSOLE_COMMAND",
            "data": "say {player} Form closed!"
        }
    ]
}