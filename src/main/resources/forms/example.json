{
    // Command
    "command": {
        // Is enable
        "enable": true,
        "name": "example",
        "description": "Example command", 
        // Command aliases
        "aliases": ["example1"],
        // Permission
        // Delete it if you don't have to
        "permission": "formcreator.example"
    },
    // Form title
    "title": "Example Form",
    // Form content
    "content": [
        "Content line"
    ],
    // Form buttons
    "buttons": [
        {
            "name": "Button",
            // Buttom image
            "image": "textures/blocks/diamond_block",
            "imageType": "path",
            // Button actions
            "actions": [
                {
                    // Available types: 
                    // PLAYER_COMMAND - Execute command from player
                    // CONSOLE_COMMAND  - Execute command from server
                    // MESSAGE - Send message to player
                    // PLAYER_MESSAGE - Send message from player
                    // OPEN_FORM - Open other form
                    // OPEN_CHEST - Open chest from ChestCreator
                    "type": "OPEN_FORM",   // Type
                    "value": "example" // Action value
                }
            ]
        },
        {
            "name": "Button 2",
            "actions": [
                {
                    "type": "PLAYER_COMMAND",
                    "value": "say Test"
                }
            ]
        }
    ],
    // Form open actions
    "openActions": [
        {
            "type": "CONSOLE_COMMAND",
            "value": "say {player} Form opened!"
        }
    ],
    // Form close actions
    "closeActions": [
        {
            "type": "CONSOLE_COMMAND",
            "value": "say {player} Form closed!"
        }
    ]
}